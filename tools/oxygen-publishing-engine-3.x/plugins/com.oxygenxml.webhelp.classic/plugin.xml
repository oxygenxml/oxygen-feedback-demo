<!DOCTYPE plugin[
 <!ENTITY commons SYSTEM 'plugin_commons.xml'>
]>
<plugin id="com.oxygenxml.webhelp.classic">
  <require plugin="org.dita.html5"/>
  <require plugin="com.oxygenxml.common"/>
  
  &commons;
  <feature extension="com.oxygenxml.classic.main.target.dependencies" value="build-init, 
    check-license, 
    check-webhelp-feedback-parameters,
    dita.map.webhelp.init, 
    preprocess,
    html5.init,
    html5.image-metadata,
    dita.out.map.webhelp.toc,
    dita.webhelp.sitemap.xml,
    copy-webhelp-resources,
    html5.css, 
    dita.topics.html.common, 
    webhelp-indexterms, 	                       
    dita.inner.topics.webhelp,
    create-main-files,
    copy-webhelp-feedback-localization,
    webhelp-index" type="text"/>
</plugin>
